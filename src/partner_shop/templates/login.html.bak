<script type="text/javascript">
function tiperr(msg){
	if(msg==1){
		msg='用户名或密码不正确！';
	}
	document.getElementById("tip").innerHTML='<span style="color:red">'+msg+'</span>';
}
function login(){
	if(j.val('username')==''){
	tiperr('请输入会员用户名');return false;	
}else if(j.val('password')==''){
	tiperr('请输入密码');return false;	
}else{j.form.asyncSubmit(0,!true);}return true;
}
